<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.tool.pomodoro.technique.tool.controller.controller.tool.ToolMenuController"
            prefHeight="25.0" prefWidth="408.0">

    <MenuBar layoutX="1.0" layoutY="2.0" prefHeight="25.0" prefWidth="408.0">
        <Menu mnemonicParsing="false" text="文件">
            <MenuItem mnemonicParsing="false" text="导入"/>
            <MenuItem mnemonicParsing="false" text="导出"/>
        </Menu>
        <Menu mnemonicParsing="false" text="编辑">
            <MenuItem onAction="#onLabelManagement" mnemonicParsing="false" text="标签管理"/>
        </Menu>
        <Menu mnemonicParsing="false" text="报表">
            <MenuItem onAction="#onReportView" mnemonicParsing="false" text="报表"/>
        </Menu>
        <Menu mnemonicParsing="false" text="帮助">
            <MenuItem mnemonicParsing="false" text="关于"/>
        </Menu>
    </MenuBar>
</AnchorPane>
